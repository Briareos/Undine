    <services>

        <service id="undine.oxygen.http_handler" class="Undine\Guzzle\Handler\CurlMultiHandler" public="false">
        </service>

        <service id="undine.oxygen.client" class="Undine\Oxygen\Client">
            <argument type="service" id="undine.oxygen.http_handler"/>
            <argument type="service" id="undine.oxygen.protocol_middleware"/>
        </service>

        <service id="undine.oxygen.protocol_middleware" class="Undine\Oxygen\Middleware\OxygenProtocolMiddleware" factory-class="Undine\Oxygen\Middleware\OxygenProtocolMiddleware" factory-method="create" public="false">
            <argument>%oxygen_module_version%</argument>
            <argument type="service" id="security.secure_random"/>
            <argument type="service" id="current_time"/>
            <argument>%oxygen_private_key_name%</argument>
            <argument>%oxygen_private_key_value%</argument>
        </service>

        <service id="undine.oxygen.login_url_generator" class="Undine\Oxygen\LoginUrlGenerator">
            <argument type="service" id="security.secure_random"/>
            <argument type="service" id="current_time"/>
        </service>

