{
    "name": "briareos/undine",
    "license": "MIT",
    "type": "project",
    "description": "Undine application for Drupal site management.",
    "autoload": {
        "psr-4": {
            "Undine\\Tests\\": "tests/",
            "Undine\\": "src/"
        },
        "files": [
            "src/Functions/json.php",
            "src/Functions/openssl.php",
            "src/Functions/string.php"
        ]
    },
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "^2.7.3",
        "doctrine/orm": "^2.5.0",
        "doctrine/dbal": "^2.5.0",
        "doctrine/doctrine-bundle": "^1.5.0",
        "symfony/swiftmailer-bundle": "^2.3.8",
        "symfony/monolog-bundle": "^2.7.1",
        "sensio/distribution-bundle": "^4.0.1",
        "sensio/framework-extra-bundle": "^3.0.10",
        "incenteev/composer-parameter-handler": "^2.1.1",
        "twig/extensions": "^1.2.0",
        "stof/doctrine-extensions-bundle": "^1.1.0",
        "guzzlehttp/guzzle": "^6.1.0",
        "knplabs/knp-menu-bundle": "^2.0.0",
        "doctrine/doctrine-migrations-bundle": "^1.0.1",
        "league/fractal": "^0.12.0",
        "omnipay/stripe": "^2.2.1",
        "jenssegers/optimus": "^0.1.3",
        "react/react": "^0.4.2"
    },
    "require-dev": {
        "sensio/generator-bundle": "^2.5.3",
        "doctrine/doctrine-fixtures-bundle": "^2.2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "npm install",
            "npm prune",
            "npm run tsd-reinstall",
            "npm run bower-install",
            "npm run bower-prune",
            "npm run gulp-build",
            "npm run gulp-build-dev",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "npm update",
            "npm prune",
            "npm shrinkwrap --dev",
            "npm run tsd-reinstall",
            "npm run bower-install",
            "npm run bower-prune",
            "npm run gulp-build",
            "npm run gulp-build-dev",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "build-parameters": "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
