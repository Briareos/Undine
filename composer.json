{
    "autoload": {
        "psr-4": {
            "Undine\\": "src/"
        },
        "files": [
            "app/AppKernel.php",
            "src/Functions/json.php",
            "src/Functions/openssl.php",
            "src/Functions/string.php",
            "src/Functions/uuid.php"
        ]
    },
    "autoload-dev": {
        "Undine\\Tests\\": "tests/"
    },
    "require": {
        "php": ">=7.0.0",
        "symfony/symfony": "^3.0.0",
        "doctrine/orm": "^2.5.0",
        "doctrine/doctrine-bundle": "^1.6.1",
        "doctrine/doctrine-cache-bundle": "^1.2.2",
        "symfony/swiftmailer-bundle": "^2.3.9",
        "symfony/monolog-bundle": "^2.8.2",
        "sensio/distribution-bundle": "^5.0.2",
        "sensio/framework-extra-bundle": "^3.0.11",
        "incenteev/composer-parameter-handler": "^2.1.2",
        "twig/extensions": "^1.3.0",
        "guzzlehttp/guzzle": "^6.1.1",
        "knplabs/knp-menu-bundle": "^2.1.0@dev",
        "league/fractal": "^0.13.0",
        "ramsey/uuid": "^3.0.1"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0.0",
        "symfony/phpunit-bridge": "^2.7.0",
        "doctrine/doctrine-fixtures-bundle": "^2.2.0"
    },
    "scripts": {
        "pre-install-cmd": [
            "Undine\\Composer\\ScriptHandler::install"
        ],
        "pre-update-cmd": [
            "Undine\\Composer\\ScriptHandler::install"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "npm install",
            "npm prune",
            "npm run gulp-build",
            "npm run gulp-build-dev",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "npm install",
            "npm prune",
            "npm run gulp-build",
            "npm run gulp-build-dev",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "build-parameters": "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
